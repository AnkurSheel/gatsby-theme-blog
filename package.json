{
    "name": "@codinators/gatsby-theme-blog",
    "private": false,
    "version": "1.4.0",
    "description": "Reusable Theme for Gatsby Blogs",
    "keywords": [
        "blog",
        "gatsby",
        "gatsby-plugin"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/gatsby-theme-blog.git"
    },
    "main": "index.js",
    "license": "MIT",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "clean": "gatsby clean",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx} config/**/*.js gatsby-*.js content/**/*.{md,mdx,markdown}",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "lint --fix",
        "lint:markdown": "markdownlint **/*.mdx --ignore node_modules/**",
        "lint:markdown:fix": "markdownlint **/*.mdx --ignore node_modules/** --fix",
        "generate-graphql": "graphql-codegen --config ./config/codegen.yml",
        "deduplicate": "yarn-deduplicate --list && yarn-deduplicate",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "dependencies": {
        "@codinators/gatsby-shared-library": "^1.1.3",
        "@emotion/babel-preset-css-prop": "^10.0.23",
        "@emotion/core": "^10.0.22",
        "@mdx-js/mdx": "^1.5.2",
        "@mdx-js/react": "^1.5.2",
        "@sindresorhus/slugify": "^0.9.1",
        "disqus-react": "^1.0.7",
        "gatsby-background-image": "^0.9.12",
        "gatsby-image": "^2.2.36",
        "gatsby-plugin-catch-links": "^2.1.21",
        "gatsby-plugin-emotion": "^4.1.18",
        "gatsby-plugin-feed": "^2.3.25",
        "gatsby-plugin-mdx": "^1.0.62",
        "gatsby-plugin-page-creator": "^2.1.37",
        "gatsby-plugin-react-helmet": "^3.1.18",
        "gatsby-plugin-robots-txt": "^1.5.0",
        "gatsby-plugin-sharp": "^2.3.8",
        "gatsby-plugin-sitemap": "^2.2.24",
        "gatsby-plugin-typescript": "^2.1.23",
        "gatsby-remark-copy-linked-files": "^2.1.33",
        "gatsby-remark-images": "^3.1.38",
        "gatsby-remark-prismjs": "^3.3.27",
        "gatsby-remark-responsive-iframe": "^2.2.30",
        "gatsby-remark-smartypants": "^2.1.19",
        "gatsby-source-filesystem": "^2.1.42",
        "gatsby-transformer-json": "^2.2.22",
        "gatsby-transformer-remark": "^2.6.42",
        "gatsby-transformer-sharp": "^2.3.9",
        "prismjs": "^1.16.0",
        "react-helmet": "^5.2.1",
        "react-kawaii": "^0.14.4",
        "ts-optchain": "^0.1.8"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@codinators/eslint-config": "^1.0.0",
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@graphql-codegen/cli": "^1.7.0",
        "@graphql-codegen/typescript": "^1.7.0",
        "@graphql-codegen/typescript-operations": "^1.7.0",
        "@semantic-release/changelog": "^3.0.6",
        "@semantic-release/git": "^7.0.18",
        "@types/react": "^16.8.25",
        "@types/react-helmet": "^5.0.9",
        "@types/react-kawaii": "^0.11.0",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "babel-loader": "^8.0.6",
        "commitizen": "^4.0.3",
        "cz-conventional-changelog": "^3.0.2",
        "eslint": "^6.3.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "gatsby": "^2.18.12",
        "gatsby-plugin-netlify": "^2.1.30",
        "graphql": "^14.5.8",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0",
        "markdownlint-cli": "^0.21.0",
        "npm-normalize-package-bin": "^1.0.1",
        "prettier": "^1.18.2",
        "puppeteer": "^2.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "semantic-release": "^15.14.0",
        "typescript": "^3.6.3",
        "webpack": "^4.41.3",
        "yalc": "^1.0.0-pre.34",
        "yarn-deduplicate": "^1.1.1"
    },
    "peerDependencies": {
        "gatsby": "^2.13.7",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
