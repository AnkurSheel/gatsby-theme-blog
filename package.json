{
    "name": "@codinators/gatsby-theme-blog",
    "private": false,
    "version": "3.12.1",
    "description": "Reusable Theme for Gatsby Blogs",
    "keywords": [
        "blog",
        "gatsby",
        "gatsby-plugin"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/gatsby-theme-blog.git"
    },
    "main": "gatsby-config.js",
    "license": "MIT",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "clean": "gatsby clean",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx} config/**/*.js gatsby-*.js content/**/*.{md,mdx,markdown}",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "yarn lint --fix",
        "lint:markdown": "markdownlint **/*.mdx --ignore node_modules/**",
        "lint:markdown:fix": "markdownlint **/*.mdx --ignore node_modules/** --fix",
        "generate-graphql": "graphql-codegen --config ./config/codegen.yml",
        "deduplicate": "yarn-deduplicate --list && yarn-deduplicate",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "dependencies": {
        "@emotion/babel-preset-css-prop": "^10.0.23",
        "@emotion/core": "^10.0.28",
        "@mdx-js/mdx": "^1.5.8",
        "@mdx-js/react": "^1.5.8",
        "@sindresorhus/slugify": "^1.0.0",
        "gatsby-background-image": "^1.0.1",
        "gatsby-image": "^2.3.1",
        "gatsby-plugin-catch-links": "^2.2.1",
        "gatsby-plugin-emotion": "^4.2.1",
        "gatsby-plugin-feed": "^2.4.1",
        "gatsby-plugin-mdx": "^1.1.4",
        "gatsby-plugin-page-creator": "^2.2.1",
        "gatsby-plugin-react-helmet": "^3.2.1",
        "gatsby-plugin-robots-txt": "^1.5.0",
        "gatsby-plugin-sharp": "^2.5.3",
        "gatsby-plugin-sitemap": "^2.3.1",
        "gatsby-plugin-typescript": "^2.3.1",
        "gatsby-remark-copy-linked-files": "^2.2.1",
        "gatsby-remark-external-links": "^0.0.4",
        "gatsby-remark-images": "^3.2.1",
        "gatsby-remark-prismjs": "^3.4.1",
        "gatsby-remark-responsive-iframe": "^2.3.1",
        "gatsby-remark-smartypants": "^2.2.1",
        "gatsby-source-filesystem": "^2.2.2",
        "gatsby-transformer-json": "^2.3.1",
        "gatsby-transformer-sharp": "^2.4.3",
        "mkdirp": "^1.0.3",
        "prismjs": "^1.16.0",
        "react-helmet": "^6.1.0",
        "react-kawaii": "^0.16.0",
        "typeface-cookie": "^1.1.13",
        "typeface-inter": "^3.12.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@codinators/eslint-config": "^1.0.0",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@graphql-codegen/cli": "^1.13.1",
        "@graphql-codegen/typescript": "^1.13.1",
        "@graphql-codegen/typescript-operations": "^1.13.1",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@types/react": "^16.9.31",
        "@types/react-helmet": "^6.1.0",
        "@types/react-kawaii": "^0.11.0",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "babel-loader": "^8.1.0",
        "commitizen": "^4.0.3",
        "cz-conventional-changelog": "^3.0.2",
        "eslint": "^7.1.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "gatsby": "^2.20.10",
        "gatsby-plugin-netlify": "^2.2.1",
        "graphql": "^15.0.0",
        "husky": "^4.2.3",
        "lint-staged": "^10.1.1",
        "markdownlint-cli": "^0.24.0",
        "npm-normalize-package-bin": "^1.0.1",
        "prettier": "^2.0.2",
        "puppeteer": "^5.2.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "semantic-release": "^17.0.4",
        "typescript": "^4.0.3",
        "webpack": "^4.42.1",
        "yalc": "^1.0.0-pre.34",
        "yarn-deduplicate": "^3.0.0"
    },
    "peerDependencies": {
        "gatsby": "^2.13.7",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
